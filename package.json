{
  "name": "react-simple-library",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://javier-machin.github.io/react-simple-library",
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "1.1.5"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.5",
    "gh-pages": "^2.0.0",
    "postcss-cli": "^6.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' src/App.css -o src/App.css",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  }
}
